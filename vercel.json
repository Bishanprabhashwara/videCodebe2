{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "be/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ],
  "installCommand": "npm --prefix be i",
  "outputDirectory": "."
}
